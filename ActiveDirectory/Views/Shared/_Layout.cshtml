<!DOCTYPE html>
<html lang="en">
<!--begin::Head-->
<head>
    <base href="">
    <meta charset="utf-8"/>
    <title>@ViewBag.title</title>
    <meta name="description" content="Metronic admin dashboard live demo. Check out all the features of the admin panel. A large number of settings, additional services and widgets."/>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"/>
    <!--begin::Fonts-->
    @Styles.Render("~/Content/View")
    @Scripts.Render("~/Content/Scripts")
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700"/>
    <link rel="shortcut icon" href="~/otokoc.svg"/>
    <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>
</head>
<!--end::Head-->
<!--begin::Body-->
<body id="kt_body" class="page-loading-enabled page-loading header-fixed header-mobile-fixed subheader-enabled subheader-fixed aside-enabled aside-fixed aside-minimize-hoverable page-loading">
@Html.Partial("~/Views/Shared/partials/_page-loader.cshtml")
<div class="d-flex flex-column flex-root">
    <!--begin::Page-->
    <div class="d-flex flex-row flex-column-fluid page">
        @Html.Partial("~/Views/Shared/partials/_aside.cshtml")
        <!--begin::Wrapper-->
        <div class="d-flex flex-column flex-row-fluid wrapper" id="kt_wrapper">
            @Html.Partial("~/Views/Shared/partials/_header.cshtml")
            <div class="content d-flex flex-column flex-column-fluid" id="kt_content">
                @Html.Partial("~/Views/Shared/partials/_subheader/subheader-v1.cshtml")
                <!--Content-->

                <div class="d-flex flex-column-fluid">
                    <!--begin::Container-->
                    <div class="container" id="laylout">

                        @RenderBody()


                    </div>

                    <!--end::Container-->
                </div>

                <!--end::Entry-->

            </div>
            <!--end::Content-->
            <!--[html-partial:include:{"file":"partials/_footer.html"}]/-->
            @Html.Partial("~/Views/Shared/partials/_footer.cshtml")
        </div>
        <!--end::Wrapper-->
    </div>
    <!--end::Page-->
</div>
@Html.Partial("~/Views/Shared/partials/_extras/offcanvas/quick_search.cshtml");
@Html.Partial("~/Views/Shared/partials/_extras/offcanvas/quick-notifications.cshtml")
@Html.Partial("~/Views/Shared/partials/_extras/scrolltop.cshtml")

<!--begin::Global Config(global config for global JS scripts)-->
<script>
    var KTAppSettings = {
        "breakpoints": {
            "sm": 576,
            "md": 768,
            "lg": 992,
            "xl": 1200,
            "xxl": 1400
        },
        "colors": {
            "theme": {
                "base": {
                    "white": "#ffffff",
                    "primary": "#3699FF",
                    "secondary": "#E5EAEE",
                    "success": "#1BC5BD",
                    "info": "#8950FC",
                    "warning": "#FFA800",
                    "danger": "#F64E60",
                    "light": "#E4E6EF",
                    "dark": "#181C32"
                },
                "light": {
                    "white": "#ffffff",
                    "primary": "#E1F0FF",
                    "secondary": "#EBEDF3",
                    "success": "#C9F7F5",
                    "info": "#EEE5FF",
                    "warning": "#FFF4DE",
                    "danger": "#FFE2E5",
                    "light": "#F3F6F9",
                    "dark": "#D6D6E0"
                },
                "inverse": {
                    "white": "#ffffff",
                    "primary": "#ffffff",
                    "secondary": "#3F4254",
                    "success": "#ffffff",
                    "info": "#ffffff",
                    "warning": "#ffffff",
                    "danger": "#ffffff",
                    "light": "#464E5F",
                    "dark": "#ffffff"
                }
            },
            "gray": {
                "gray-100": "#F3F6F9",
                "gray-200": "#EBEDF3",
                "gray-300": "#E4E6EF",
                "gray-400": "#D1D3E0",
                "gray-500": "#B5B5C3",
                "gray-600": "#7E8299",
                "gray-700": "#5E6278",
                "gray-800": "#3F4254",
                "gray-900": "#181C32"
            }
        },
        "font-family": "Poppins"
    };
</script>
<!--end::Global Config-->
<!--begin::Global Theme Bundle(used by all pages)-->
<!--end::Global Theme Bundle-->
<!--begin::Page Vendors(used by this page)-->
<script src="~/Scripts/Addons/Main.js"></script>
@RenderSection("scripts", false)


<!--end::Page Scripts-->
<div class="modal fade bd-example-modal-xl" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" id="GlobalModal">
    <div class="modal-dialog modal-xl">
        <div class="modal-content" id="RenderCrate">
        </div>
    </div>
</div>
</body>
</html>